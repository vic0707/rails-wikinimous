<h1>List of Articles</h1>

<ul>
	<% @articles.each do |article|%>
	<li> <%= link_to article.title, article_path(article) %> <%= link_to "Edit", edit_path(article) %> <%= link_to "Delete", delete_path(article), method: :delete %></li>
	<% end %>
</ul>

<%= link_to "Create a new Article", new_path  %>

